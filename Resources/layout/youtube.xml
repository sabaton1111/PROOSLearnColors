<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/background_levels"
    android:minWidth="25px"
    android:minHeight="25px">

    <android.webkit.WebView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/webviewYoutube"
        android:layout_marginBottom="200px"
        android:layout_marginTop="50px"
        android:layout_marginLeft="50px"
        android:layout_marginRight="50px"
    />
    <ImageView
        android:background="@mipmap/home_icon"
        android:layout_width="150px"
        android:layout_height="150px"
        android:scaleType="centerCrop"
        android:layout_below="@+id/imageViewLeftBox"
        android:adjustViewBounds="true"
        android:id="@+id/imageViewHomeButtonYoutube"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="20px"
        android:layout_marginLeft="100px"/>
    <ImageView
        android:background="@mipmap/skip"
        android:layout_width="200px"
        android:layout_height="300px"
        android:scaleType="centerCrop"
        android:layout_below="@+id/imageViewLeftBox"
        android:adjustViewBounds="true"
        android:id="@+id/imageViewSkip"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="-50px"
        android:layout_marginRight="50px"/>
              
</RelativeLayout>
